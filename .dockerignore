target/
!.mvn/wrapper/maven-wrapper.jar
!**/src/main/**/target/
!**/src/test/**/target/

# IDE
.idea/
.vscode/
*.swp
*.swo
*~
.DS_Store

# Environment files
.env
.env.local
.env.*.local

# Logs
logs/
*.log

# Git
.git/
.gitignore

# Documentation
*.md
!README.md

# Docker
Dockerfile
docker-compose.yml
.dockerignore

# Test coverage
coverage/
.nyc_output/

# Temporary files
tmp/
temp/
